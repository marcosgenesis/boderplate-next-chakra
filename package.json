{
  "name": "boderplate-frontend",
  "version": "0.1.0",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix",
      "cross-env CI=true yarn test --bail --findRelatedTests",
      "git add"
    ]
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@chakra-ui/core": "^0.8.0",
    "@chakra-ui/react": "^1.6.10",
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "framer-motion": "^4.1.17",
    "next": "11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/node": "^16.11.4",
    "@types/react": "^17.0.31",
    "cross-env": "^7.0.3",
    "eslint": "8.0.1",
    "eslint-config-next": "11.1.2",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.3",
    "typescript": "^4.4.4"
  }
}
